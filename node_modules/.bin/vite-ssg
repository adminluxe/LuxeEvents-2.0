#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tontoncestcarre/luxeevents-frontend/node_modules/.pnpm/vite-ssg@28.0.0_unhead@2.0.12_vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1._60b4f4c8eaee39a84e9f3b21607a24a0/node_modules/vite-ssg/bin/node_modules:/home/tontoncestcarre/luxeevents-frontend/node_modules/.pnpm/vite-ssg@28.0.0_unhead@2.0.12_vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1._60b4f4c8eaee39a84e9f3b21607a24a0/node_modules/vite-ssg/node_modules:/home/tontoncestcarre/luxeevents-frontend/node_modules/.pnpm/vite-ssg@28.0.0_unhead@2.0.12_vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1._60b4f4c8eaee39a84e9f3b21607a24a0/node_modules:/home/tontoncestcarre/luxeevents-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tontoncestcarre/luxeevents-frontend/node_modules/.pnpm/vite-ssg@28.0.0_unhead@2.0.12_vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1._60b4f4c8eaee39a84e9f3b21607a24a0/node_modules/vite-ssg/bin/node_modules:/home/tontoncestcarre/luxeevents-frontend/node_modules/.pnpm/vite-ssg@28.0.0_unhead@2.0.12_vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1._60b4f4c8eaee39a84e9f3b21607a24a0/node_modules/vite-ssg/node_modules:/home/tontoncestcarre/luxeevents-frontend/node_modules/.pnpm/vite-ssg@28.0.0_unhead@2.0.12_vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1._60b4f4c8eaee39a84e9f3b21607a24a0/node_modules:/home/tontoncestcarre/luxeevents-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
else
  exec node  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
fi
